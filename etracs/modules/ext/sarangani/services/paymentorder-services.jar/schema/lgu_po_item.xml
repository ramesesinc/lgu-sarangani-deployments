<schema adapter="po">
	<element tablename="po_item">
		<field name="objid" primary="true" prefix="POI" />
		<field name="parent_objid" require="true" />
		<field name="item_objid" require="true" />
		<field name="item_code" require="true" />
		<field name="item_title" require="true" />
		<field name="amount" require="true" type="decimal" />
	</element>
</schema>