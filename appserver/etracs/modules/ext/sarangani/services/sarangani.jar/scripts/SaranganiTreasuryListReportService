import com.rameses.common.*
import com.rameses.annotations.*

class SaranganiTreasuryListReportService {
	
	@ActiveDB('sarangani_treasury_collection_list')
	def em

	@Service('RPTUtil')
	def util

	@ProxyMethod
	public def getList(params) {
		println 'params => ' + params 
		
		params.collectorid = '%'
		if (params.collector){
			params.collectorid = params.collector.objid
		}

		if (params.period != 'range') {
			util.buildStartEndDateFilter(params)
		}

		return em.getList(params)
	}

}