<schema adapter="checkissuance">
	<element tablename="checkbook">
		<field name="objid" primary ="true" prefix="CB"/>
		<field name="state" required="true" caption="State" defaultValue="DRAFT" indexed="true"/>
		<field name="bankaccount_objid" required="true"/>
		<field name="bankaccount_code" required="true" caption="Account No." searchable="true" indexed="true"/>
		<field name="bankaccount_title" required="true" caption="Account Title"/>
		<field name="stubno" required="true" caption="Stub No." searchable="true"/>
		<field name="startseries" required="true" caption="Start Series" type="integer"/>
		<field name="endseries" required="true" caption="End Series" type="integer"/>
		<field name="currentseries" required="true" caption="Current Series" type="integer"/>
		<field name="assignedto_objid"/>
		<field name="assignedto_name" caption="Assigned To" searchable="true" indexed="true"/>
		<field name="assignedto_title"/>
		
	</element>
</schema>